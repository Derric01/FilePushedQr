{
  "name": "filepushedqr",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/src/server.ts",
    "dev:frontend": "next dev -H 0.0.0.0",
    "build": "next build && tsc -p backend/tsconfig.json",
